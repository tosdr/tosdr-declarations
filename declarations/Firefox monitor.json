    },
    "Privacy Policy": {
      "fetch": "https://www.mozilla.org/en-US/privacy/firefox-monitor/",
      "select": "html > body > div:nth-of-type(3) > div > article"
    },
    "Terms of Service": {
      "fetch": "https://www.mozilla.org/en-US/about/legal/terms/services/",
      "select": "body"
    }
  }
}
